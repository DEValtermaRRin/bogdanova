{
  "extends": "airbnb-base",
  "rules": {
    "no-underscore-dangle": ["error", { "allow": ["_id"]}],
    "import/prefer-default-export": "off",
    "default-case":"off",
    "no-console": "off",
    "no-plusplus": "off",
    "no-param-reassign":"off",
    "no-unused-expressions": "off",
    "import/extensions": "off"
  },
  "globals": {
    "document": false
  },
  "env": {
    "browser": true,
    "node": true
  }
}